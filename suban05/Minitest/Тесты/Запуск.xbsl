@Обработчик
метод ПриОткрытииПоСсылке(Событие: СобытиеПриОткрытииПоСсылке)
    знч Результаты = новый Массив<Строка>()
    для Модуль из Тестирование.ПолучитьСервисы()
        знч Набор = новый НаборТестов()
        Модуль.Тесты(Набор)
        Набор.Запустить()
        Результаты.Добавить(
            [Модуль.ВСтроку(), Набор.Результаты.Соединить("\н")].Соединить("\н")
        )
    ;
    Компоненты.Результаты.Значение = Результаты.Соединить("\н\н")
;