@ВПодсистеме
метод Запустить(): Массив<РезультатТестирования>
    знч Результаты = новый Массив<РезультатТестирования>()
    для Модуль из Тестирование.ПолучитьСервисы()
        знч Набор = новый НаборТестов()
        Модуль.Тесты(Набор)
        Набор.Запустить()
        Результаты.Добавить(
            новый РезультатТестирования(
                Набор.Статус,
                Модуль.ВСтроку(),
                Набор.Результаты
            )
        )
    ;
    возврат Результаты
;